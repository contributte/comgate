{
  "name": "contributte/comgate",
  "description": "Comgate Payment Gateway for Nette Framework",
  "keywords": [
    "comgate",
    "payment",
    "php",
    "api",
    "nette"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/comgate",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "brick/money": "^0.5.0",
    "guzzlehttp/guzzle": "^6.3.3|^7.0",
    "guzzlehttp/psr7": "^1.7"
  },
  "require-dev": {
    "mockery/mockery": "~1.3.0",
    "nette/di": "~2.4.15 || ~3.0.0",
    "ninjify/nunjuck": "^0.3.0",
    "ninjify/qa": "^0.12.1",
    "phpstan/phpstan": "^0.12.2",
    "phpstan/phpstan-deprecation-rules": "^0.12.0",
    "phpstan/phpstan-nette": "^0.12.0",
    "phpstan/phpstan-strict-rules": "^0.12.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Comgate\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Cases\\": "tests/cases"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.3.x-dev"
    }
  }
}
